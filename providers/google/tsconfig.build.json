{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "incremental": true,
    "noEmit": false,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": false,
    "exactOptionalPropertyTypes": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noUncheckedIndexedAccess": false,
    "isolatedModules": true
  }
}
